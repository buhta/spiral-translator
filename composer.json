{
  "name": "spiral/translator",
  "type": "library",
  "description": "Translator, Static Analysis and Auto-Indexation",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "spiral/core": "^1.0",
    "spiral/logger": "^1.0",
    "spiral/tokenizer": "^2.0",
    "symfony/translation": "4.2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1"
  },
  "autoload": {
    "files": [
      "functions.php"
    ],
    "psr-4": {
